
  <h2>Explore Nearby</h2>


    <div id='map'></div>

    <div class="owl-carousel">
      <% @photos_with_geo.each do |photo| %>
        <div><%= image_tag photo.images.low_resolution.url, :id => "#{photo.location.latitude}_#{photo.location.longitude}"%></div>
      <% end %>
    </div>
      <script>$(".owl-carousel").owlCarousel();</script>
